{
    "name": "sheinfeld/invisible-recaptcha",
    "description": "Invisible reCAPTCHA For Laravel.",
    "keywords": ["recaptcha", "no-captcha", "captcha", "invisible", "invisible-recaptcha", "laravel", "laravel5", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert.cht@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6.4 || ^7.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "guzzlehttp/guzzle": "^5.3.1|^6.2.1|^7.0"
    },
    "require-dev": {
        "illuminate/view": "^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sheinfeld\\InvisibleReCaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sheinfeld\\InvisibleReCaptcha\\InvisibleReCaptchaServiceProvider"
            ]
        }
    }
}
